  "main": "index.js",
  "scripts": {
    "test": "mocha --require babel-register test/**/*.js --recursive",
    "test:watch": "mocha --require babel-register test/**/*.js --recursive --watch",
    "karma": "karma start",
    "start": "webpack-dev-server --open",
    "dev": "webpack-dev-server --env.dev",
		"build": "webpack",
		"deploy": "npm run build && firebase deploy",
    "docs": "rmdir /S docs & jsdoc -c jsdoc.json",
    "fake:data-server": "json-server --watch spot_db.json --port 3000",
    "fake:core-server": "json-server --watch core_db.json --port 3001"
  },
